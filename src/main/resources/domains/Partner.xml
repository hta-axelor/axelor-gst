<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="Partner"> 
    <string name="gstin" title="GSTIN"/>
    <string name="contactType" title="Contact Type" selection="gst.contact.type.selection" />
    <one-to-one name="secondaryEmailAddress" ref="com.axelor.apps.message.db.EmailAddress" title="Secondary Email"/>
    
        <extra-code><![CDATA[
		  public static final String CONTACT_PRIMARY = "1";
		  public static final String CONTACT_SECONDARY = "2";
		  public static final String CONTACT_OTHERS = "3";
		]]></extra-code>
  </entity>

</domain-models>
