            <xml-property name="queryText"><![CDATA[select MetaFile.file_path as logo_path, Address.full_name, Partner.web_site, Email.address as email, Partner.fax, Partner.fixed_phone
from auth_user as AuthUser 
left outer join base_company as Company on (AuthUser.active_company = Company.id)
left outer join base_address as Address on (Company.address = Address.id)
left outer join base_partner as Partner on (Company.partner = Partner.id)
left outer join message_email_address as Email on (Email.id = Partner.email_address)
left outer join meta_file as MetaFile on (MetaFile.id = Company.logo)
where AuthUser.id = ?]]></xml-property>
